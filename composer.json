{
    "name": "seyi-adedokun/saloodo-test",
    "description": "Saloodo REST API with LUMEN + Authentication",
    "keywords": ["laravel", "lumen", "jwt", "saloodo"],
    "authors": [
        {
            "name": "Seyi Adedokun",
            "email": "adedokunoluwaseyi@gmail.com"
        }
    ],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=7.1",
        "barryvdh/laravel-cors": "^0.11",
        "laravel/lumen-framework": "5.8.*",
        "tymon/jwt-auth": "dev-develop",
        "vlucas/phpdotenv": "~3.3"
    },
    "require-dev": {
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": "~1.0",
        "phpunit/phpunit": "~7.0",
        "squizlabs/php_codesniffer": "^3.1"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/",
            "database/"
        ]
    },
    "scripts": {
        "post-root-package-install": [
            "php -r \"copy('.env.example', '.env');\""
        ]
    },
    "config": {
        "sort-packages": true,
        "optimize-autoloader": true
    }
}
